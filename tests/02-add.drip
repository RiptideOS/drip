//! This example showcases math operations, function calls, experession syntax,
//! and string interpolation using the `print` intrinsic function

fn main() {
    let a = 2;
    let one = {
        let b = -1;
        a + b
    };
    
    print("Result: {}\n", max(add(one + 1, 2), 1));
}

fn add(a: i64, b: i64) -> i64 {
    a + b 
}

fn max(a: i64, b: i64) -> i64 {
    if a > b {
        a
    } else {
        b
    }
}
